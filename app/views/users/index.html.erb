<%= render 'books/userinfo', user: @user, book: @book %>

<h1>Users</h1>

<% @users.each do |user| %>

    <p>image</p>
        <% if user.profile_image.attached? %>
        <%= image_tag user.profile_image, size: "200x200" %>
      <% else %>
        <%= image_tag 'no_image', size: "200x200" %>
      <% end %>
    <p>name</p>
    <%= user.name %>
    <p><%= link_to 'show',user_path(user.id) %></p>

<% end %>